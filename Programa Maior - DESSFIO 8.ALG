algoritmo "Programa Maior - Dessfio 8"

  var n1, n2: real

 inicio
//Aba de recepção
escreval("Olá, Seja Bem Vindo(a)")
escreval("")

//Aba onde digita-se os dois números para a potência
  escreval("Digite um número")
  leia(n1)
escreval("Digite um número")
  leia(n2)

se (n1>n2) entao
escreval("O maior número é ",n1)
senao
escreval("O maior número é ",n2)
fimse

  fimalgoritmo