algoritmo "Formatação"
var

inicio
  Escreval("A 2:6 ", pi:2:6, " A")
  Escreval("A 2:5 ", pi:2:5, " A")
  Escreval("A 2:4 ", pi:2:4, " A")
  Escreval("A 2:3 ", pi:2:3, " A")
  Escreval
  Escreval("A 2:2 ", pi:2:2, " A")
  Escreval("A 3:2 ", pi:3:2, " A")
  Escreval("A 4:2 ", pi:4:2, " A")
  Escreval("A 5:2 ", pi:5:2, " A")
  Escreval("A 6:2 ", pi:6:2, " A")
fimalgoritmo

// Atalhos
// F8 = Debugar ( Executa passo a passo )
// F9 = Executar
// CTRL + C = Copiar
// CTRL + T = Selecionar tudo
// CTRL + V = Colar
// CTRL + X = Recortar
// ! depois CTRL + espaço = Esboço do algoritmo
